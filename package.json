{
  "name": "corner-radius",
  "version": "0.0.0-development",
  "description": "Draw SVG paths under the union of an element's and its childrens' polygons.",
  "repository": {
    "type": "git",
    "url": "https://github.com/tvanc/corner-radius.git"
  },
  "license": "MIT",
  "author": "Travis Van Couvering",
  "sideEffects": false,
  "main": "dist/",
  "module": "src/",
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "semantic-release": "semantic-release",
    "test": "jest --coverage && yarn lint --watch",
    "dev-server": "encore dev-server",
    "dev": "encore dev",
    "ci": "rm -rf node_modules && yarn install --frozen-lockfile"
  },
  "devDependencies": {
    "@symfony/webpack-encore": "^1.5.0",
    "core-js": "^3.15.2",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.1.0",
    "prettier": "2.2.1",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "semantic-release": "17.4.1",
    "semantic-release-cli": "5.4.3",
    "ts-loader": "^9.0.0",
    "typescript": "^4.3.5",
    "webpack-notifier": "^1.6.0"
  },
  "renovate": {
    "extends": [
      "config:js-lib",
      ":automergePatch",
      ":automergeBranch",
      ":automergePatch",
      ":automergeBranch",
      ":automergeLinters",
      ":automergeTesters",
      ":automergeTypes"
    ]
  }
}
