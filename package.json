{
  "name": "corner-radius",
  "version": "0.0.0-development",
  "private": false,
  "description": "Draw SVG paths under the union of an element's and its childrens' polygons.",
  "repository": {
    "type": "git",
    "url": "https://github.com/tvanc/corner-radius.git"
  },
  "license": "MIT",
  "author": "Travis Van Couvering",
  "sideEffects": false,
  "main": "dist/",
  "module": "lib/",
  "files": [
    "dist/",
    "lib/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "babel lib/ -d dist/ --source-maps --ignore '**/*.test.js' --delete-dir-on-start",
    "prepare": "yarn build",
    "semantic-release": "semantic-release",
    "live": "live-server --open=lib/example.html",
    "test": "jest --coverage && yarn lint --watch"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "12"
          }
        }
      ]
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "semi": false
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.13.0",
    "@babel/core": "7.13.8",
    "@babel/preset-env": "7.13.9",
    "@types/jest": "26.0.20",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.10",
    "jest": "26.6.3",
    "jest-date-mock": "1.0.8",
    "live-server": "^1.2.1",
    "prettier": "2.2.1",
    "prettier-plugin-packagejson": "2.2.10",
    "semantic-release": "17.4.1",
    "semantic-release-cli": "5.4.3"
  },
  "renovate": {
    "extends": [
      "config:js-lib",
      ":automergePatch",
      ":automergeBranch",
      ":automergePatch",
      ":automergeBranch",
      ":automergeLinters",
      ":automergeTesters",
      ":automergeTypes"
    ]
  }
}
